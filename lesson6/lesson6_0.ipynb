   "source": [
    "#溫柔版\n",
    "\n",
    "try:\n",
    "    num = int(input('請輸入一個正整數'))\n",
    "except:\n",
    "    print('格式錯誤')\n",
    "else:\n",
    "    num2 = num // 2\n",
    "\n",
    "    print(f'{num} 的因數有:')\n",
    "\n",
    "    list = []\n",
    "\n",
    "    for i in range(1, num2 + 1):\n",
    "        if num % i == 0:\n",
    "            list.append(str(i))\n",
    "\n",
    "    list.append(str(num))\n",
    "\n",
    "    print(', '.join(list))"
   ]
  }
